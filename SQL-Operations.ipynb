   "source": [
    "2.SELECT State, District, Year, MAX(Rape) AS Max_Rapes, MAX(Kidnapping_Abduction) AS Max_Kidnappings\n",
    "FROM crimes_against_women\n",
    "GROUP BY State, District, Year\n",
    "ORDER BY Max_Rapes DESC, Max_Kidnappings DESC\n",
    "LIMIT 1;\n"
   ]
  },
  {
   "cell_type": "raw",
   "id": "864819a6",
   "metadata": {},
   "source": [
    "3.SELECT District, MAX(Dacoity_Robbery) AS Max_Dacoity_Robbery\n",
    "FROM crimes_against_women\n",
    "GROUP BY District\n",
    "ORDER BY Max_Dacoity_Robbery DESC\n",
    "LIMIT 1;\n"
   ]
  },
