   "metadata": {},
   "source": [
    "4.SELECT District, MIN(Murder) AS Min_Murders\n",
    "FROM crimes_against_women\n",
    "GROUP BY District\n",
    "ORDER BY Min_Murders ASC;\n"
   ]
